{
    "name": "add-multifile",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.22.0"
    },
    "main": "./out/extension.js",
    "scripts": {
        "watch": "tsc -b -w"
    },
    "packageManager": "yarn@3.2.4",
    "devDependencies": {
        "@types/vscode": "^1.72.0",
        "@typescript-eslint/eslint-plugin": "^5.41.0",
        "@typescript-eslint/parser": "^5.41.0",
        "eslint": "^8.26.0",
        "typescript": "^4.8.4"
    },
    "activationEvents": [
        "onStartupFinished"
    ],
    "contributes": {
        "menus": {
            "explorer/context": [
                {
                    "when": "explorerResourceIsFolder == true",
                    "command": "multifile.add",
                    "group": "navigation@7"
                }
            ]
        },
        "commands": [
            {
                "command": "multifile.add",
                "title": "New Multi-File Item..."
            }
        ]
    }
}
